<tr>
  <td> <%= answer.body%></td>
  <td> <%= answer.correct%></td>
  <td> <%= button_to  'Show', answer_path(answer),method: :get %></td>
  <td> <%= button_to "Delete", answer_path(answer), method: :delete, data: {confirm: 'Are you sure?'}%><td/>
  <td> <%= button_to  "Edit", edit_answer_path(answer),method: :get %></td>
</tr>